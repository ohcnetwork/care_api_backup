meta {
  name: login
  type: http
  seq: 1
}

post {
  url: {{url}}/api/v1/auth/login/
  body: json
  auth: none
}

body:json {
  {
      "username": "{{username}}",
      "password": "{{password}}"
  }
}

script:post-response {
  let data = res.getBody();
  bru.setEnvVar("access_token",data.access);
}
